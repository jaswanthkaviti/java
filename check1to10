import java.util.Scanner;
public class check{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		while(n < 1 || n> 10) {
			System.out.print(n +" is not between 1 to 10 and try again:");
			n = input.nextInt();
		}
		System.out.println(n +"is between 1 to 10");
		input.close();
	
	}
}